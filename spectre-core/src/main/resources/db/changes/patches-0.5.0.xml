<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">


    <changeSet id="202602091101" author="vudsen">
        <dropTable tableName="static_permission"/>
    </changeSet>

    <changeSet id="202602091445" author="vudsen">
        <update tableName="policy_permission" >
            <column name="resource" value="runtime-node"/>
            <where>resource = :value</where>
            <whereParams>
                <param value="runtime_node"/>
            </whereParams>
        </update>
    </changeSet>
</databaseChangeLog>
